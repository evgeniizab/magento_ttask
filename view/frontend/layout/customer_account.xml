<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
        <referenceContainer name="sidebar.main">
                <referenceBlock name="customer_account_navigation">
                    <block class="Magento\Framework\View\Element\Html\Link\Current" name="my-projects-link">
                        <arguments>

                            <argument name="path" xsi:type="string">/ttask/index/display</argument>
                            <argument name="label" xsi:type="string" translate="true">My Projects</argument>

                        </arguments>
                    </block>
                    <block class="Magento\Framework\View\Element\Html\Link\Current" name="my-task-link">
                        <arguments>

                            <argument name="path" xsi:type="string">/ttask/index/display</argument>
                            <argument name="label" xsi:type="string" translate="true">My Tasks</argument>

                        </arguments>
                    </block>
                </referenceBlock>
        </referenceContainer>
</page>
